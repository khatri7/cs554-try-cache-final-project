{
	"name": "client",
	"version": "1.0.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true
		},
		"extends": [
			"plugin:react/recommended",
			"airbnb",
			"airbnb/hooks",
			"plugin:prettier/recommended"
		],
		"settings": {
			"import/resolver": {
				"node": {
					"moduleDirectory": [
						"src/",
						"node_modules"
					],
					"extensions": [
						".js",
						".jsx"
					]
				}
			}
		},
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"plugins": [
			"react",
			"prettier"
		],
		"rules": {
			"react/prop-types": "off",
			"no-underscore-dangle": "off",
			"react/jsx-props-no-spreading": "off"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
